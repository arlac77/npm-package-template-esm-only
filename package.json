{
  "name": "{{name}}",
  "version": "0.0.0-semantic-release",
  "description": "{{description}}",
  "main": "{{module}}",
  "module": "{{module}}",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "cover": "#overwrite c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "test": "ava",
    "docs": "documentation readme {{module}} --section=API",
    "lint": "documentation lint {{module}}"
  },
  "dependencies": {},
  "devDependencies": {
    "documentation": "^12.1.4",
    "ava": "^2.4.0",
    "c8": "^7.0.0",
    "esm": "^3.2.25",
    "semantic-release": "^15.14.0"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=12.14.0"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/npm-package-template-root",
      "arlac77/npm-package-template-travis"
    ],
    "files": [
      {
        "merger": "Package",
        "pattern": "**/package.json",
        "options": {
          "keywords": {
            "auth-provider": "auth-provider",
            "repository-provider.+": "repository-provider",
            "^kronos-interceptor.+": "kronos-interceptor",
            "^kronos-service-(?!manager)": "kronos-service",
            "^kronos-step.+": "kronos-step",
            "^kronos-adapter.+": "kronos-step"
          }
        }
      },
      {
        "merger": "Readme",
        "pattern": "README.md",
        "options": {
          "badges": [
            {
              "name": "styled with prettier",
              "icon": "https://img.shields.io/badge/styled_with-prettier-ff69b4.svg",
              "url": "https://github.com/prettier/prettier"
            },
            {
              "name": "codecov.io",
              "icon": "http://codecov.io/github/{{github.user}}/{{github.repo}}/coverage.svg?branch=master",
              "url": "http://codecov.io/github/{{github.user}}/{{github.repo}}?branch=master"
            },
            {
              "name": "Maintainability",
              "icon": "https://api.codeclimate.com/v1/badges/{{id}}/maintainability",
              "url": "https://codeclimate.com/github/{{github.user}}/{{github.repo}}/maintainability"
            },
            {
              "name": "Known Vulnerabilities",
              "icon": "https://snyk.io/test/github/{{github.user}}/{{github.repo}}/badge.svg",
              "url": "https://snyk.io/test/github/{{github.user}}/{{github.repo}}"
            },
            {
              "name": "Commitizen friendly",
              "icon": "https://img.shields.io/badge/commitizen-friendly-brightgreen.svg",
              "url": "http://commitizen.github.io/cz-cli/"
            }
          ]
        }
      }
    ],
    "usedBy": [
      "Kronos-Integration/endpoint",
      "Kronos-Integration/interceptor",
      "Kronos-Integration/interceptor-decode-json",
      "Kronos-Integration/interceptor-gzip",
      "Kronos-Integration/service",
      "Kronos-Integration/service-admin",
      "Kronos-Integration/service-authenticator",
      "Kronos-Integration/service-consul",
      "Kronos-Integration/service-health-check",
      "Kronos-Integration/service-http",
      "Kronos-Integration/service-ldap",
      "Kronos-Integration/service-logger-gelf",
      "Kronos-Integration/service-systemd",
      "Kronos-Integration/service-uti",
      "Kronos-Integration/test-interceptor",
      "Kronos-Integration/test-step",
      "arlac77/aggregation-repository-provider",
      "arlac77/bitbucket-repository-provider",
      "arlac77/cache-repository-provider",
      "arlac77/chunk-tokenizer",
      "arlac77/config-expander",
      "arlac77/console-auth-provider",
      "arlac77/content-entry",
      "arlac77/env-auth-provider",
      "arlac77/es6-modules-playground",
      "arlac77/expression-expander",
      "arlac77/fs-resolver-fs",
      "arlac77/gd-playground",
      "arlac77/gitea-repository-provider",
      "arlac77/github-repository-provider",
      "arlac77/hinted-tree-merger",
      "arlac77/iterable-string-interceptor",
      "arlac77/keytar-auth-provider",
      "arlac77/koa-github-hook-handler",
      "arlac77/kv-reader",
      "arlac77/local-repository-provider",
      "arlac77/loglevel-mixin",
      "arlac77/mock-repository-provider",
      "arlac77/model",
      "arlac77/model-attributes",
      "arlac77/mqtt-timeseries-leveldb",
      "arlac77/multi-path-matcher",
      "arlac77/npm-dep-graph",
      "arlac77/npm-package-walker",
      "arlac77/pratt-parser",
      "arlac77/registry-mixin",
      "arlac77/remove-sensible-values",
      "arlac77/repository-provider",
      "arlac77/repository-provider-test-support",
      "arlac77/rpm-codec",
      "arlac77/scope-reporter",
      "arlac77/sftp-resolver-fs",
      "arlac77/statetransition-mixin",
      "arlac77/svn-dav-fs",
      "arlac77/svn-repository-provider",
      "arlac77/svn-simple-auth-provider",
      "arlac77/url-resolver-fs",
      "arlac77/uti",
      "arlac77/x-expression",
      "k0nsti/konsum-db"
    ]
  }
}
